{
  "root": "apps/front",
  "sourceRoot": "apps/front/src",
  "projectType": "application",
  "tags": ["apps:front"],
  "targets": {
    "prepare": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "./",
        "parallel": false,
        "commands": [{ "command": "rm -rf dist/apps/front" }, { "command": "mkdir -p dist/apps/front" }]
      }
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "apps/front",
        "parallel": false,
        "commands": [{ "command": "npx nx run front:prepare" }, { "command": "NODE_ENV=development npm run build" }]
      },
      "configurations": {
        "preview": {
          "commands": [
            { "command": "npx nx run front:prepare" },
            { "command": "NODE_ENV=preview npm run build" },
            { "command": "compress-cra -d dist/apps/front" }
          ]
        },
        "production": {
          "commands": [
            { "command": "npx nx run front:prepare" },
            { "command": "NODE_ENV=production npm run build" },
            { "command": "compress-cra -d dist/apps/front" }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "apps/front",
        "parallel": false,
        "commands": [{ "command": "npm run start" }]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/front/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "apps/front",
        "parallel": false,
        "commands": [{ "command": "npm run test -- --watchAll=false" }]
      }
    }
  }
}
